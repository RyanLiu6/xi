{
  "name": "@ryanliu6/xi",
  "version": "1.0.0",
  "type": "module",
  "description": "Project Xi is a component library for my personal Astro + Tailwind projects.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
