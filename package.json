      "types": "./dist/components/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.3.12",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "classnames": "^2.5.1",
    "tailwindcss": "^3.4.15"
  }
}
